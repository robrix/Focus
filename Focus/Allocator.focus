	Interface composedOf: [#pages, #nurseryPage],
	Interface composedOf: [#frames],
	Interface composedOf: [#allocateObject]
] as: {
	#allocateObject = {
		self nurseryPage allocateObject else: {
			// perform a collection and reallocate
		}
	}
}
